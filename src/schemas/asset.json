{
    "$schema": "http://json-schema.org/draft-07/schema#",
	"$id": "http://ciam.nestle.com/schemas/data.json#",
    "title": "asset",
	"description": "data/asset subschema",
	"type": "object",
    "definitions": {
        "asset": {
            "type": "object",
            "properties": {
                "applicationInternalIdentifier": {
                    "type": "string"
                },
                "assetSerialNb": {
                    "type": "string"
                },
                "assetType": {
                    "type": "string"
                },
                "brand": {
                    "type": "string"
                },
                "creationDate": {
                    "type": "string",
                    "format": "date-time"
                },
                "details": {
                    "type": "string"
                },
                "isDeleted": {
                    "type": "boolean"
                },
                "lastUpdateDate": {
                    "type": "string",
                    "format": "date-time"
                },
                "model": {
                    "type": "string"
                },
                "purchaseDate": {
                    "type": "string",
                    "format": "date"
                },
                "registrationDate": {
                    "type": "string",
                    "format": "date"
                },
                "registrationReferenceID": {
                    "type": "string"
                },
                "source": {
                    "type": "string"
                },
                "warrantyEndDate": {
                    "type": "string",
                    "format": "date"
                }
            },
            "required": [
                "applicationInternalIdentifier"
            ]
        }
    }
}